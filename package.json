{
  "name": "MuuCMS",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint --ext .jsx,.js src",
    "lint:fix": "eslint --ext .jsx,.js --fix src",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "start": "cross-env NODE_ENV=local BABEL_ENV=local babel-watch src/backend --presets es2015,stage-0,stage-2",
    "stop": "pm2 kill",
    "test": "node scripts/test.js src --env=jsdom",
    "test:coverage": "node scripts/test.js src --coverage --env=jsdom"
  },
  "author": "Carlos Santana",
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTestFramework.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/browserMocks.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "buffer-node": "1.0.4",
    "compression-webpack-plugin": "2.0.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "css-loader": "1.0.0",
    "date-format": "1.2.0",
    "dot-object": "1.7.0",
    "express": "4.16.3",
    "express-handlebars": "3.0.0",
    "express-session": "1.15.6",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "font-awesome": "^4.7.0",
    "highlight.js": "9.12.0",
    "html-minifier": "3.5.20",
    "https": "1.0.0",
    "jquery": "^3.3.1",
    "moment": "2.22.2",
    "mysql": "2.16.0",
    "node-sass": "^4.9.3",
    "node-sass-middleware": "^0.11.0",
    "node-time-ago": "1.0.0",
    "normalize-css": "^2.3.1",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-twitter": "1.0.4",
    "pm2": "3.0.4",
    "prop-types": "15.6.2",
    "query-string": "6.1.0",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.6",
    "react-lazyload": "2.3.0",
    "react-nl2br": "^0.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redis": "2.8.0",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "1.5.0",
    "style-loader": "0.23.0",
    "tinymce": "4.8.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.2",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-watch": "2.0.7",
    "cross-env": "5.2.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-babel": "5.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.5.0",
    "jsdom": "12.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-mock-store": "1.5.3"
  }
}
